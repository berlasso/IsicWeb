<?xml version="1.0" encoding="utf-8"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <system.web>    
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    
  </system.web>
  
  <system.serviceModel>
    <behaviors>
      <endpointBehaviors>
        <behavior name="WsdlEndpointBehavior">
          <wsdlExtensions location="https://sistemas.mpba.gov.ar/ISICServices/RenaperResponseService.svc" singleFile="true" xdt:Locator="Condition(@singleFile='true')" xdt:Transform="Replace"/>
        </behavior>
      </endpointBehaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="false" xdt:Locator="Condition(@httpGetEnabled='true')"  xdt:Transform="Replace"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>

  </system.serviceModel>
</configuration>