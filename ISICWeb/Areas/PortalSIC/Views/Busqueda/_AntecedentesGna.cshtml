@model ISIC.Entities.Prontuario


<div class="tile tile  wow fadeInDown animated" style="margin-bottom: 80px;">

    <div class="panel panel-default ">
        <div class="panel-heading">
            <h4>
                Listado de fichas de GNA
            </h4>
        </div>
    </div>

    <div class="panel-body">
        <div class="row">
            <div class="col-md-12">
                <table class="table  table-bordered table-hover  table-striped  tabla" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>
                                Apellido
                            </th>
                            <th>
                                Nombre
                            </th>
                            <th>
                                Documento Nro.
                            </th>
                            <th>
                                Caratula
                            </th>

                        </tr>
                    </thead>
                    <tbody>
                        @if (Model != null && Model.DatosGNA != null)
                        {
                            foreach (var item in Model.DatosGNA.Where(x => x.Baja != true))
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Apellido)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Nombre)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.DocumentoNumero)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Caratula)
                                    </td>

                                </tr>
                            }
                        }

                    </tbody>
                </table>
                <div class="row">
                    <div class="col-md-12">
                        <div class="pull-right">

                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>



</div>
