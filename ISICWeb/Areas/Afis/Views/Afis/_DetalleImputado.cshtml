@model ISIC.Entities.Imputado

@{
    ViewBag.Title = "DetalleImputado";
}

<div class="panel panel-default tile wow fadeInDown animated">
    <div class="panel-heading">
        <h5>Datos según el SIC</h5>
    </div>

    <div class="panel-body">
        <div class="row">
            <div class="col-md-2 col-sm-2">
                @Html.LabelFor(model => model.Prontuario.ProntuarioNro, "Prontuario")
                @Html.EditorFor(model => model.Prontuario.ProntuarioNro, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                
            </div>
            <div class="col-md-3 col-sm-3">
                @Html.LabelFor(model => model.Persona.Apellido, "Apellido")
                @Html.EditorFor(model => model.Persona.Apellido, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                

            </div>
            <div class="col-md-3 col-sm-3">
                @Html.LabelFor(model => model.Persona.Nombre, "Nombre")
                @Html.EditorFor(model => model.Persona.Nombre, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
            <div class="col-md-2 col-sm-2">
                @Html.LabelFor(model => model.Persona.DocumentoNumero, "Documento Nro.")
                @Html.EditorFor(model => model.Persona.DocumentoNumero, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
            <div class="col-md-2 col-sm-2">
                @Html.LabelFor(model => model.Persona.Sexo.descripcion, "Sexo")
                @Html.EditorFor(model => model.Persona.Sexo.descripcion, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
    </div>
</div>