@model ISIC.Entities.Imputado

@{
    ViewBag.Title = "Detalles";
}


<br>

<div class="alert alert-info">
    <strong>Detalles de Imputado</strong>
</div>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.ProntuarioSIC)
